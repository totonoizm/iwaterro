<%= form_with model:@post_gym, local:true do |f| %>
        <%= f.attachment_field :image %>

        <%= f.text_field :gym_name, class:"form-control", placeholder:"gym名" %>
        <%= f.text_field :caption, class:"form-control", placeholder:"gym名" %>
        <%= f.submit "送信", class:"btn btn-lg btn-block btn-base-1 mt-20" %>

<% end %>
